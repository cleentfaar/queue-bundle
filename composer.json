{
  "name": "treehouselabs/queue-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 bundle for message queue",
  "keywords": ["mq","message queue","message","queue","amqp","rabbitmq","worker","pubsub"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "require": {
    "php":                 ">=5.5",
    "psr/log":             "^1.0",
    "symfony/symfony":     "^2.6|^3.0",
    "treehouselabs/queue": "^v0.0.4"
  },
  "require-dev": {
    "ext-amqp":                     "^1.7",
    "doctrine/orm":                 "^2.3",
    "phpunit/phpunit":              "^4.0",
    "behat/behat":                  "dev-master as 3.0",
    "symfony/proxy-manager-bridge": "^2.4",
    "ocramius/proxy-manager":       "^0.5"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\QueueBundle\\": "src/TreeHouse/QueueBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\QueueBundle\\Tests\\": "tests/src/TreeHouse/QueueBundle/",
      "TreeHouse\\FunctionalTestBundle\\": "tests/src/TreeHouse/FunctionalTestBundle/"
    }
  }
}
